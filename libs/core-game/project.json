{
  "name": "core-game",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core-game/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/core-game",
        "tsConfig": "libs/core-game/tsconfig.lib.json",
        "packageJson": "libs/core-game/package.json",
        "main": "libs/core-game/src/index.ts",
        "assets": ["libs/core-game/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/core-game"],
      "options": {
        "jestConfig": "libs/core-game/jest.config.ts"
      }
    }
  }
}
