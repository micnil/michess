          "value": "(\n        \"games\".\"time_control_classification\" NOT IN ('bullet', 'blitz', 'rapid')\n        OR (\n          \"games\".\"time_control\" IS NOT NULL\n          AND \"games\".\"time_control\"->>'initial' IS NOT NULL\n          AND \"games\".\"time_control\"->>'increment' IS NOT NULL\n        )\n      )"
