{
  "name": "michess",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:web": "nx serve web-chess",
    "start:node": "nx serve node-chess | pino-pretty",
    "start:db": "dotenv -e ./apps/node-chess/.env -c -- docker-compose up -d postgres redis",
    "clean:db": "docker-compose down -v && pnpm run start:db && pnpm run migration:run",
    "build": "nx build",
    "test": "nx test",
    "test:all": "nx run-many --target=test --all",
    "test:all:coverage": "nx run-many --target=test --all --coverage",
    "typecheck": "pnpm run typecheck:web && pnpm run typecheck:node",
    "typecheck:web": "tsc --build apps/web-chess --noEmit",
    "typecheck:node": "tsc --build apps/node-chess --noEmit",
    "perft": "tsx --tsconfig tools/tsconfig.tools.json tools/perft.ts",
    "perft:debug": "tsx --inspect-brk --tsconfig tools/tsconfig.tools.json tools/perft.ts",
    "migration:generate": "drizzle-kit generate --config=libs/infra-db/drizzle.config.ts",
    "better-auth:generate": "pnpx @better-auth/cli generate --config=libs/infra-db/auth.config.ts --output=libs/infra-db/src/lib/schema/auth.ts",
    "drizzle-studio": "dotenv -e ./apps/node-chess/.env -c -- drizzle-kit studio --config=libs/infra-db/drizzle.config.ts",
    "migration:run": "dotenv -e ./apps/node-chess/.env -c -- drizzle-kit migrate --config=libs/infra-db/drizzle.config.ts",
    "migration:push": "dotenv -e ./apps/node-chess/.env -c -- drizzle-kit push --config=libs/infra-db/drizzle.config.ts --force"
  },
  "private": true,
  "dependencies": {
    "@hono/node-server": "^1.19.0",
    "@hono/zod-validator": "^0.7.2",
    "@radix-ui/themes": "^3.2.1",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-router": "^1.131.41",
    "better-auth": "^1.3.8",
    "bullmq": "^5.58.9",
    "core-js": "^3.25.1",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.4",
    "ky": "^1.10.0",
    "lucide-react": "^0.544.0",
    "pino": "^9.9.5",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@angular-devkit/core": "^20.0.0",
    "@nx/cypress": "21.5.2",
    "@nx/esbuild": "21.5.2",
    "@nx/eslint": "21.5.2",
    "@nx/eslint-plugin": "21.5.2",
    "@nx/jest": "21.5.2",
    "@nx/js": "21.5.2",
    "@nx/node": "21.5.2",
    "@nx/react": "21.5.2",
    "@nx/vite": "21.5.2",
